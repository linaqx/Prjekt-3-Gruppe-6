
@{
    ViewBag.Title = "FindAllEntertainments";
}

<h2>FindAllEntertainments</h2>
<table class="table">
    <tr>
        <th>
            @Html.DisplayName("Dato")
        </th>
        <th>
            @Html.DisplayName("Name")
        </th>
       
        <th></th>
    </tr>

    @foreach ()
    {
        <tr>
            <td>
                @item.TimeSlot.StartTime.ToString("dd/MM/yyyy HH:mm")
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Title)
            </td>
            <td>
                @Html.ActionLink("Aflys", "Delete", new { id = item.TimeSlotID })
            </td>
        </tr>

